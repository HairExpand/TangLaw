<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <OutputPath>bin\</OutputPath>
    <Nullable>enable</Nullable> <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="templates\content.html" />
    <None Remove="templates\index.html" />
    <None Remove="templates\template.html" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="templates\content.html" />
    <EmbeddedResource Include="templates\index.html" />
    <EmbeddedResource Include="templates\template.html" />
  </ItemGroup>

</Project>
